{
   "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "allowJs": true,
      "strict": true,
      "esModuleInterop": true
   },
   "include": ["src/**/*"],
   "exclude": ["node modules","dist"]
}